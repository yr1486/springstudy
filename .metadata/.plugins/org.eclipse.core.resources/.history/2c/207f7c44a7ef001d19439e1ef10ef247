<!-- namespace는 @Mapper를 지정한다. -->
<mapper namespace="com.gdu.staff.mapper.StaffMapper">

	<select id="getStaffList" resultType="StaffDTO">
		SELECT SNO, NAME, DEPT, SALARY
		  FROM STAFF
		 ORDER BY SNO DESC
	</select>
	
</mapper>